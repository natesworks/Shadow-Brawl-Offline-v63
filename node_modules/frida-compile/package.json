{
  "name": "frida-compile",
  "version": "19.0.4",
  "description": "Compile a Frida script comprised of one or more Node.js modules",
  "keywords": [
    "frida"
  ],
  "homepage": "https://frida.re",
  "type": "module",
  "bin": {
    "frida-compile": "dist/cli.js"
  },
